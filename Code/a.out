.data
_prompt: .asciiz "Enter an integer:"
_ret: .asciiz "\n"
.globl main
.text
read:
  li $v0, 4
  la $a0, _prompt
  syscall
  li $v0, 5
  syscall
  jr $ra

write:
  li $v0, 1
  syscall
  li $v0, 4
  la $a0, _ret
  syscall
  move $v0, $0
  jr $ra

func :
  addi $sp, $sp, -12
main :
  move $fp, $sp
  addi $sp, $sp, -12
  lw $t0, -8($fp)
  sw $t0, -12($fp)
  lw $t0, -12($fp)
  sw $t0, -4($fp)
